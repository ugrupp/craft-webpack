{
  "require": {
    "aelvan/imager": "^2.3",
    "carlcs/craft-redactorcustomstyles": "^3.0",
    "craftcms/cms": "^3.0.0",
    "craftcms/redactor": "^2.4",
    "ether/seo": "^3.6",
    "mmikkel/cp-field-inspect": "^1.0",
    "nystudio107/craft-emptycoalesce": "^1.0",
    "nystudio107/craft-twigpack": "^1.1",
    "nystudio107/craft-typogrify": "^1.1",
    "sebastianlenz/linkfield": "^1.0",
    "vardump/recentchanges": "^1.1",
    "verbb/expanded-singles": "^1.0",
    "verbb/field-manager": "^2.1",
    "verbb/smith": "^1.0",
    "verbb/super-table": "^2.3",
    "vlucas/phpdotenv": "^2.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
